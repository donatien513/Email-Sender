{
  "version": 2,
  "name": "go",
  "public": true,
  "builds": [
    {
      "src": "*.go",
      "use": "@now/go"
    }
  ],
  "routes": [
    { "src": "/", "dest": "main.go" }
  ],
  "env": {
    "username": "@username",
    "password": "@password",
    "hostname": "@hostname",
    "port": "@port"
  }
}
